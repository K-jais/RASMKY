                        <div style="padding:23px 40px" class="col-md-9">
                            <p style="color:#2196f3;font-size: 26px;font-family: 'Roboto Slab', serif;"><%= restaurant.name %></p>
                            <p style="color:#fff;font-size: 16px;font-family: 'Roboto Slab', serif;"><%= restaurant.location.address %></p>
                            <span style="color:#fff;font-size:13px"><span style="color:#fff;font-size:16px;padding:2px 5px;background-color: #2196f3 "><i class="fa fa-star me-1" aria-hidden="true"></i><%= restaurant.user_rating.aggregate_rating %></span>  (500+ ratings)</span>
                            <span style="margin-left: 40px;color:#fff;font-size:15px"><i style="margin-right: 40px;font-size: 4px;" class="fa fa-circle" aria-hidden="true"></i><i class="fa fa-inr me-1" aria-hidden="true"></i><%= restaurant.average_cost_for_two %> For Two</span>
                            <p style="color:#fff;font-size: 16px;font-family: 'Roboto Slab', serif;margin-top: 18px;"><%= restaurant.cuisines %></p>
                            <p style="color:#fff;font-size: 14px;font-family: 'Roboto Slab', serif;margin-top: 18px;">Timings :   <span style="color: #2196f3;"><%= restaurant.timings %></span></p>
                        </div>
                    </div>
            </div>
            </div> 
        </section>
        <section id="cart">
          <div class="container">
            <div class="row">
              <div class="col-md-7">
                <div style="min-height:300px;background-color: #fff;border:2.5px solid #413e66;border-radius: 18px;" class="hello2">
                  <h1 style="margin-bottom: 17px;margin:0px 50px;padding-bottom: 6px;border-bottom: 2.5px solid #2196f3;margin-top:15px;text-align: center;color:#2196f3;font-family: 'Roboto Slab', serif;font-size: 32px;margin-bottom: 30px;"><i class="fa fa-shopping-cart me-3" aria-hidden="true"></i>Cart</h1>
                  <div style="margin:0px 50px"class="hello3">
                    <% for(let i=1;i< user.cart.carting.length;i++){ %>
                    <p class="items-list">
                      <span class="items-type" style="float:left;"><span class="type-type" name="<%= user.cart.carting[i].type %>" style="color:green;margin-right:10px;font-size:18px"><i class='fa fa-dot-circle-o' aria-hidden='true'></i></span><span style="color:#413e66;font-family:'Roboto Slab', serif;font-size:20px"><%= user.cart.carting[i].name %></span><span class="quantity-quantity" style="color:#2196f3;font-family:'Roboto Slab', serif;font-size:20px;margin-left:6px"><i class="fa fa-times me-1" aria-hidden="true"></i><%= user.cart.carting[i].quantity %></span></span>
                      <span class="items-price" style="float:right;color:#413e66;font-size: 20px;font-family:'Roboto Slab',serif;"><i class="fa fa-inr me-1" aria-hidden="true"></i><span><%= user.cart.carting[i].price %></span></span>
                      <br>
                    </p>
                    <%}%>
                  </div>
                </div>
              </div>
              <div class="col-md-5">
                <div style="min-height:300px;background-color: #fff;border:2.5px solid #413e66;border-radius: 18px;padding-bottom: 15px;" class="hello2">
                  <h1 style="margin-bottom: 17px;margin:0px 40px;padding-bottom: 6px;border-bottom: 2.5px solid #2196f3;margin-top:15px;text-align: center;color:#2196f3;font-family: 'Roboto Slab', serif;font-size: 32px;margin-bottom: 30px;"><i class="fa fa-credit-card-alt me-3" aria-hidden="true"></i>Bill Details</h1>
                  <div style="margin:0px 40px" class="bill">
                    <p>
                      <span style="float:left;color:#413e66;font-family:'Roboto Slab',serif;font-size: 20px;"><span>Item Total</span></span>
                      <span id="total_price" style="color:#413e66;font-family:'Roboto Slab',serif;font-size: 20px;"><span style="float:right"><i class="fa fa-inr me-1" aria-hidden="true"></i><span>600</span></span></span>
                      <br>
                    </p>
                    <p style="padding-bottom: 15px;border-bottom: 1.5px solid #413e66;">
                      <span style="float:left;color:#2196f3;font-family:'Roboto Slab',serif;font-size: 20px;"><span>Delivery Partner Fee</span></span>
                      <span id="delivery_fee" style="color:#413e66;font-family:'Roboto Slab',serif;font-size: 20px;"><span style="float: right;"><i class="fa fa-inr me-1" aria-hidden="true"></i><span>24</span></span></span>
                      <br>
                    </p>
                    <p style="padding-bottom: 15px;border-bottom: 1.5px solid #413e66;">
                      <span style="float:left;color:#2196f3;font-family:'Roboto Slab',serif;font-size: 20px;"><span>Taxes & Charges</span></span>
                      <span id="tax_price" style="color:#413e66;font-family:'Roboto Slab',serif;font-size: 20px;"><span style="float:right"><i class="fa fa-inr me-1" aria-hidden="true"></i><span>32</span></span></span>
                      <br>
                    </p>
                    <p>
                      <span style="float:left;color:#413e66;font-family:'Roboto Slab',serif;font-size: 20px;"><span>To Pay</span></span>
                      <span id="total_total" style="color:#413e66;font-family:'Roboto Slab',serif;font-size: 20px;"><span style="float:right"><i class="fa fa-inr me-1" aria-hidden="true"></i><span>656.35</span></span></span>
                      <br>
                    </p>
                  </div>
                </div>
              </div>
            </div>
            <div style="display: flex;justify-content: center;align-items: center;">
              <form action="/removecart">
                <button class="button2">Remove all Items from Cart</button>
              </form>
            </div>
          </div>
        </section>
        <section style="margin-bottom: 60px;" id=addresses>
          <div class="container">
            <h1 style="color:#2196f3;font-family: 'Roboto Slab',serif;font-size: 30px;margin-bottom: 30px;"><i class="fa fa-address-card-o me-3" aria-hidden="true"></i>Choose a delivery address</h1>
            <div style="margin-bottom: 40px;" class="row">
              <% for(let i=0;i< user.address.length;i++){ %>
              <div style="margin:20px 0px" class="col-md-4">
                <div class="card h-100 hello">
                  <div class="card-body">
                    <h5 style="color:#2196f3;font-family: 'Roboto Slab',serif;" class="card-title"><%= user.address[i].name %></h5>
                    <p style="color:#413e66;font-family: 'Roboto Slab',serif;" class="card-text"><%= user.address[i].add %></p>
                    <div>
                      <input id="<%= i %>" class="offset" type="hidden" value="0">
                      <button class="butt">SELECT</button>
                    </div>
                  </div>
                </div>
              </div>
              <%}%>
              <div style="margin:20px 0px" class="col-md-4">
                <a style="text-decoration: none;" href="/profile"><div class="card h-100 hello">
                  <div class="card-body">
                    <h5 style="text-align: center;color:#2196f3" class="card-title">Add new address</h5>
                    <div style="display: flex;justify-content: center;align-items: center;">
                      <i style="font-size: 28px;color:#413e66" class="fa fa-plus-circle" aria-hidden="true"></i>
                    </div>
                  </div>
                </div></a>
              </div>
            </div>
            <h1 style="color:#2196f3;font-family: 'Roboto Slab',serif;font-size: 30px;margin-bottom: 60px;"><i class="fa fa-money me-3" aria-hidden="true"></i>Payment : <span style="font-size:23px;color:#413e66;margin-left:20px">Cash on Delivery</span></h1>
            <div style="display: flex;justify-content: center;align-items: center;">
              <form action='/tracking' method="POST">
                <input type="hidden" id="restaurant_details" name="restaurant_details" value="<%= JSON.stringify(restaurant) %>">
                <input type="hidden" id="address_details" name="address_details">
                <input type="hidden" id="user_details" name="user_details" value="<%= JSON.stringify(user) %>">
                <input type="hidden" id="totalprice_details" name="totalprice_details">
                <input type="hidden" id="date_date" name="date_date">
                <input type="hidden" id="date_time" name="date_time">
                <button class="button-place" >Place Your Order</button>              
              </form>
            </div>
            
          </div>

        </section>
        <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js" integrity="sha384-ygbV9kiqUc6oa4msXn9868pTtWMgiQaeYH7/t7LECLbyPA2x65Kgf80OJFdroafW" crossorigin="anonymous"></script>
        <script>
          var totalprice=0;
          var id='0';
          var address="";
          $('#'+id).parent('div').children('.butt').css('background','#413e66');
          $('#'+id).parent('div').children('.butt').css('border','2px solid #413e66');
          $('#'+id).parent('div').children('.butt').text('SELECTED');
          address=$('#'+id).parent('div').parent('div').children('p').text();
          console.log(address);
          $('#'+id).attr('value','1');
          
          $('.items-list').each(function(){
            var color=$(this).children('.items-type').children('.type-type').attr('name');
            $(this).children('.items-type').children('.type-type').css('color',color);
            var single=$(this).children('.items-price').children('span').text();
            var quantity=$(this).children('.items-type').children('.quantity-quantity').text();
            var singleprice=parseInt(single);
            var quantityprice=parseInt(quantity);
            singleprice=singleprice*quantityprice;
            totalprice=totalprice+singleprice;
            var textprice=singleprice.toString();
            $(this).children('.items-price').children('span').text(textprice);
            console.log($(this).children('.items-price').children('span').text());
          });
          var total_price=totalprice.toString();
          $('#total_price').children('span').children('span').text(total_price);
          var delivery_fee=parseInt($('#delivery_fee').children('span').children('span').text());
          var tax_price=parseInt($('#tax_price').children('span').children('span').text());
          totalprice=totalprice+delivery_fee+tax_price;
          total_price=totalprice.toString();
          $('#total_total').children('span').children('span').text(total_price);
          console.log($('#total_total').children('span').children('span').text());

          $('.butt').click(function(){
            var offset=$(this).parent('div').children('.offset').attr('value');
            if(offset == 0)
            {
              $('#'+id).parent('div').children('.butt').css('background','#2196f3');
              $('#'+id).parent('div').children('.butt').css('border','2px solid #2196f3');
              $('#'+id).parent('div').children('.butt').text('SELECT');
              $('#'+id).attr('value','0');
              address="";
              $(this).css('background','#413e66');
              $(this).css('border','2px solid #413e66');
              $(this).text('SELECTED');
              address=$(this).parent('div').parent('div').children('p').text();
              console.log(address);
              $(this).parent('div').children('.offset').attr('value','1');
              id=$(this).parent('div').children('.offset').attr('id');
            }
          });
          $('.button-place').click(function(){
            $('#address_details').attr('value',address);
            $('#totalprice_details').attr('value',totalprice);
            var currentdate = new Date(); 
            var date_date =  currentdate.getDate() + "/"
                + (currentdate.getMonth()+1)  + "/" 
                + currentdate.getFullYear();
            var date_time =  currentdate.getHours() + ":"  
                + currentdate.getMinutes() + ":" 
                + currentdate.getSeconds();
            $('#date_date').attr('value',date_date);
            $('#date_time').attr('value',date_time);
          });
        </script>
    </body>
</html>
